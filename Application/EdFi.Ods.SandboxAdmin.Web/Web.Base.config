         connectionString="Server=(local); Database=EdFi_{0}; Trusted_Connection=True; Application Name=EdFi.Ods.SandboxAdmin.Web;"
         providerName="System.Data.SqlClient" />
  </connectionStrings>
  <initialization configSource="AdminCredential.config" />
  <!-- For more information on log4net configuration, visit https://logging.apache.org/log4net/release/manual/configuration.html -->
  <log4net>
    <appender name="TraceAppender" type="log4net.Appender.TraceAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline" />
      </layout>
    </appender>
    <appender name="FileAppender" type="log4net.Appender.FileAppender">
      <file value="${ProgramData}\Ed-Fi-ODS-Admin\Log.txt" />
      <appendToFile value="false" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline" />
      </layout>
    </appender>
    <root>
      <level value="INFO" />
      <appender-ref ref="TraceAppender" />
      <appender-ref ref="FileAppender" />
    </root>
    <logger name="NHibernate">
      <level value="WARN" />
    </logger>
  </log4net>
